
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">

        <title>{% block title %}Gestion Carnet d'adresse{% endblock %} </title>        
        <!-- Tell the browser to be responsive to screen width -->
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        {% block stylesheets %}
            <!-- Bootstrap 3.3.5 -->  
            <link rel="stylesheet" href="{{ asset('css/bootstrap/css/bootstrap.min.css') }}"/>    
            {# <link rel="stylesheet" href="{{ asset('bundles/espritappgestion/css/bootstrap/css/Bootstrap v3.3.6.min.css') }}"/>  <!--  mehrez modif  -->
            <link rel="stylesheet" href="{{ asset('bundles/espritappgestion/css/bootstrap/css/Bootstrap v3.3.6 bootstrap-theme.min.css') }}"/>  <!--  mehrez modif  -->  #}
            <!-- Font Awesome -->
            <link rel="stylesheet" href="{{ asset('css/fontawesome/css/font-awesome.min.css') }}"/>  
            <!-- Ionicons -->
            <link rel="stylesheet" href="{{ asset('css/font-awesome-ionicons/ionicons.min.css') }}"/> 
            <!-- Theme style -->
            <link rel="stylesheet" href="{{ asset('css/dist/css/AdminLTE.css') }}"/>
            <!-- AdminLTE Skins. Choose a skin from the css/skins
                 folder instead of downloading all of them to reduce the load. -->
            <link rel="stylesheet" href="{{ asset('css/dist/css/skins/skin-yellow-light.min.css') }}"/> 
            <!-- checkbox and radio -->
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/plugins/ICheck/all.css') }}"/>       
            <!-- add * to required fields -->
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/bootstrap/css/RequiredFields.css') }}"/>       
            <!-- languages flag -->
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/bootstrap/css/languages.min.css') }}"/>                   
            <!--  Bootstrap style nouveaux fichiers asset:install -->


        {% endblock %}
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="sidebar-mini skin-yellow-light fixed">

        <div class="wrapper">

            <header class="main-header">
                {% block logo %}
                    <!-- Logo -->
                    <a href="" class="logo">
                        <!-- mini logo for sidebar mini 50x50 pixels -->
                        <span class="logo-mini">
                            <b>Carnet</b>
                        </span>
                        <!-- logo for regular state and mobile devices -->
                        <span class="logo-lg">
                            {# <img src="{{ asset('bundles/espritappgestion/images/CGA-trans.png') }}" width="70%"/> #}
                            <b>Gestion</b>Carnet d'Adresse
                        </span>

                        <!-- <span class="logo-mini"><b>CGA</b></span>                  
                       <span class="logo-lg"><b>Comié Général CGA</b></span> -->
                    </a>
                {% endblock %}
                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top" role="navigation">
                    <!-- Sidebar toggle button-->
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <!-- Messages: style can be found in dropdown.less-->
                            {% block messages %}                                
                            {% endblock %}
                            <!-- Notifications: style can be found in dropdown.less -->
                            {% block notifications %}                                
                            {% endblock %}
                            {% block tasks %}                                   
                            {% endblock %}
                            <!-- User Account: style can be found in dropdown.less -->
                            {% block users %}
                                <li class="dropdown user user-menu">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <img src="{{ asset('images/user_profil.png') }}" class="user-image" alt="User Image">
                                        <span class="hidden-xs">  {% if app.user%}{{app.user.username}}  {% endif %}</span>
                                    </a>
                                    <ul class="dropdown-menu">
                                        <!-- User image -->
                                        <li class="user-header">
                                            <img src="{{ asset('images/user_profil.png') }}" class="img-circle" alt="User Image" />
                                            <p>


                                            </p>
                                        </li>
                                        <!-- Menu Footer-->
                                        <li class="user-footer">
                                            {#<div class="pull-left">
                                                <a href="#" class="btn btn-default btn-flat">Profile</a>
                                            </div>#}
                                            <div class="pull-right">
                                                <a href="{{path('fos_user_security_logout')}}" class="btn btn-default btn-flat">Sign out</a>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                            {% endblock %}
                        </ul>
                    </div>

                </nav>
            </header>

            <!-- Left side column. contains the logo and sidebar -->
            <aside class="main-sidebar">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">

                    <!-- sidebar menu: : style can be found in sidebar.less -->
                    <ul class="sidebar-menu">
                        {% block sidebar %} 

                            <li {% if app.request.get('_route') == 'AppartooParisGestionIndex' or app.request.get('_route') == 'AppartooParisGestionEdit' %} class="active"{% endif %}><a href="{{path('AppartooParisGestionIndex')}}"><i class="fa fa-users"></i> <span>Gestion membres</span></a></li>                


                        {% endblock %}
                    </ul>

                </section>
                <!-- /.sidebar -->
            </aside>

            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    {% block header_content %}
                    {% endblock %}
                </section>

                <!-- Main content -->
                <section class="content">
                    <!-- Info boxes -->
                    <div class="row">
                        {% block content %}

                        {% endblock %}
                    </div>




                </section><!-- /.content -->
            </div><!-- /.content-wrapper -->

            <footer class="main-footer">
                {% block footer %}
                    <div class="pull-right hidden-xs">
                        <b>Version</b> 1.0
                    </div>
                    <strong>Copyright &copy; 2015-2016 <a href="">Gouti</a>.</strong> All rights reserved.
                {% endblock %}
            </footer>

            {% block javascript %}


                <!-- jQuery 2.1.4 -->
                <script src="{{ asset('js/plugins/jQuery/jQuery-2.1.4.min.js') }}" type="text/javascript"></script>   
                <!--   return    On   Top     -->
                {#    <script src="{{ asset('bundles/espritappgestion/js/returnOnTop.js') }}" type="text/javascript"></script>  #}
                <!-- Bootstrap 3.3.5 -->
                <script src="{{ asset('js/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
                <!-- FastClick -->
                <script src="{{ asset('js/plugins/fastclick/fastclick.min.js') }}" type="text/javascript"></script>       
                <!-- AdminLTE App -->
                <script src="{{ asset('js/dist/js/app.min.js') }}" type="text/javascript"></script>
                <!-- SlimScroll 1.3.0 -->
                <script src="{{ asset('js/plugins/slimScroll/jquery.slimscroll.min.js') }}" type="text/javascript"></script>       
                <!-- checkbox and radio -->
                <script src="{{ asset('js/plugins/ICheck/icheck.min.js') }}" type="text/javascript"></script>       
                <script>
                    //Flat red color scheme for iCheck
                    $('input[type="checkbox"].flat-red, input[type="radio"].flat-red').iCheck({
                        checkboxClass: 'icheckbox_flat-green',
                        radioClass: 'iradio_flat-green'
                    });
                </script>
                <script>
                    $(document).ready(function () {
                        $('body').append('<div id="returnOnTop" title="Retour en haut">&nbsp;</div>');
                        $('#returnOnTop').click(function () {
                            $('html,body').animate({scrollTop: 0}, 'slow');
                        });
                    });
                    $(window).scroll(function () {
                        if ($(window).scrollTop() === 0)
                            $('#returnOnTop').fadeOut();
                        else
                            $('#returnOnTop').fadeIn();
                    });
                </script>



            {% endblock %}
    </body>
</html>


